page{
  background-color: rgb(248, 248, 248);
}

.userint{
  padding: 25rpx;
  padding-top: 15rpx;
  margin-bottom: 10rpx;
  background-color: rgb(255, 255, 255);
}

.userProfile{
  flex-direction: row;
  width: 700rpx;
  display: flex;
  align-items: center;
  height: 90rpx;
  justify-content: flex-start;
}

.userPortrait{
  display: flex;
  align-items: center;
}

.userAvatar{
  width: 75rpx;
  height: 75rpx;
  border-radius: 50%;
  margin-right: 10rpx;
}

.uNameAndInfo{
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.uName{
  color: rgb(88, 107, 143);
  height: 40rpx;
  font-size: 29rpx;
  font-weight: 610;
  line-height: 50rpx;
}

.udate{
  color: rgb(145, 145, 148);
  font-size: 24rpx;
}

.head{
  font-size: 38rpx;
  margin-left:25rpx ;
}

.uPlace{
  font-size: 32rpx;
  margin-bottom: 4rpx;
}

.uArticle{
  font-size: 32rpx;
  margin-bottom: 4rpx;
}

.uType{
  font-size: 32rpx;
  margin-bottom: 4rpx;
}

.uContact{
  font-size: 32rpx;
  margin-bottom: 10rpx;
}

.uSection{
  font-size: 32rpx;
  margin-bottom: 10rpx;
  margin-top: 15rpx;
}

.noticePic{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: row;
}

.userPic{
  margin-right: 10rpx;
  margin-left: 0;
  margin-top: 0;
  margin-bottom: 0;
  width: 220rpx;
  height: 220rpx;
  border-radius: 5px;
}

.end{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
margin-top:30vh ;
}

.end image{
width: 300rpx;
height: 300rpx;
}

.end text{
color: rgb(199, 199, 199);
  }

